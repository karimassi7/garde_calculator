int test()
{
    int tests;
 do{
    printf("enter the number of your tests");
    scanf("%d",&tests);}while(tests<1 || tests>10);
return tests;
}
float calcul_TOTALEOfScorres(float scores,int tests)
{
    float total_scores=0;
    for(int i=0;i<=tests;i++)
    return total_scores=scores++;
}
//calcul average=tatale_sccores/tests.
float CALCULE_average(float scores,int tests)
{
    float average=0;
float total_scores=calcul_TOTALEOfScorres(float scores,int tests);
return average=total_scores/tests;
}
char getLetterGrade(float average)
{
    if(average<=100|| average>=90 ) return A;
    else if (average<=89 || average>=80) return B;
    else if (average<=79 || average>=70) return C;
    else if(average<=69 || average>=60) return F;
    else return E;
}
//now we will go to the main function
void main()
{
    int test=test();
    float scores;
    do{
        printf("enter the scores of your tests");
        scanf("%f",&scores)}while(scores<0 || scores>100);
    float total_scores=calcul_TOTALEOfScorres(float scores,float test);
    float average=CALCULE_average(float scores,int test);
    char resultat=getLetterGrade(float average);
    printf("The average is %f and the grade letter is %c",average,resulat);
}